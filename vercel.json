{
  "buildCommand": "cd client && npm install --legacy-peer-deps && npm run build",
  "outputDirectory": "client/dist",
  "rewrites": [
    {
      "source": "/api/roads/:id/ratings",
      "destination": "/api/roads/[id]/ratings"
    },
    {
      "source": "/api/roads",
      "destination": "/api/roads"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ]
}
